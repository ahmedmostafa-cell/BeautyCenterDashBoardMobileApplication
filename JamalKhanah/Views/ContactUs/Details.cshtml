@model ContactUs

@{
    ViewData["Title"] = "تفاصيل معلومات التواصل ";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
<div class="row">						
    <div class="col-xl-8">
        <div class="card m-b-20">
            <div class="card-header">
                <h3 class="card-title"> تفاصيل عن معلومات الاتصال </h3>
            </div>
            <div class="card-body mb-0">
                    
                <dl class="row">
                    <dt class = "col-sm-2">
                        @Html.DisplayNameFor(model => model.WhatsAppNumber)
                    </dt>
                    <dd class = "col-sm-10">
                        @Html.DisplayFor(model => model.WhatsAppNumber)
                    </dd>
                    <dt class = "col-sm-2">
                        @Html.DisplayNameFor(model => model.Email)
                    </dt>
                    <dd class = "col-sm-10">
                        @Html.DisplayFor(model => model.Email)
                    </dd>
                    <dt class = "col-sm-2">
                        @Html.DisplayNameFor(model => model.Link)
                    </dt>
                    <dd class = "col-sm-10">
                        @Html.DisplayFor(model => model.Link)
                    </dd>
                    <dt class = "col-sm-2">
                        @Html.DisplayNameFor(model => model.FaceBookLink)
                    </dt>
                    <dd class = "col-sm-10">
                        @Html.DisplayFor(model => model.FaceBookLink)
                    </dd>
                    <dt class = "col-sm-2">
                        @Html.DisplayNameFor(model => model.PhoneNumber)
                    </dt>
                    <dd class = "col-sm-10">
                        @Html.DisplayFor(model => model.PhoneNumber)
                    </dd>
                    <dt class = "col-sm-2">
                        @Html.DisplayNameFor(model => model.TermsAndConditions)
                    </dt>
                    <dd class = "col-sm-10">
                        @Html.DisplayFor(model => model.TermsAndConditions)
                    </dd>
                    <dt class = "col-sm-2">
                        @Html.DisplayNameFor(model => model.TermsAndConditionsAr)
                    </dt>
                    <dd class = "col-sm-10">
                        @Html.DisplayFor(model => model.TermsAndConditionsAr)
                    </dd>
                </dl>
            </div>
        </div>
    </div>
</div>

<div style="margin-bottom: 4rem;text-align: end;">
    <a asp-action="Edit" asp-route-id="@Model?.Id"class="btn btn-warning">تعديل</a> 
    <a asp-action="Index"class="btn btn-secondary">العودة للقائمة</a>
</div>
