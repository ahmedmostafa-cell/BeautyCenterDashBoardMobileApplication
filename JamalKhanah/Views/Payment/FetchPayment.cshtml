@using System.Globalization;
@model Moyasar.Services.Payment
@{
    Layout = null;
    string backgroundImage = "url('data:image/png;base64,')";
    if (!ViewBag.IsSuccess)
    {
        backgroundImage = "url('data:image/png;base64,')";
    }
}

<!DOCTYPE html>
<html>
<head>
    <!-- Meta data -->
    <meta charset="UTF-8">
    <meta name='viewport' content='width=device-width, initial-scale=1.0, user-scalable=0'>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- Title -->
    <title>JamalKhanah </title>

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Cairo:wght@600&display=swap" rel="stylesheet">
    <style>
        .paymt-message {
            text-align: center;
            margin: 30px auto;
            max-width: 300px;
        }

        .paymt-message__figure {
            width: 150px;
            height: 150px;
            margin: 0 auto 30px;
            background-repeat: no-repeat;
            background-size: cover;
            background-position: center;
            background: @Html.Raw(backgroundImage);
        }

        .paymt-message__text {
            text-transform: uppercase;
            font-size: 22px;
            margin-bottom: 10px;
            color: @(ViewBag.IsSuccess ? "green" : "red")
        }

        * {
            font-family: Cairo;
        }

        td:first-child {
            width: 35%;
            text-align: right;
        }

        td:nth-child(2){
            width:75%;
        }
    </style>
</head>
<body>
    <div class="paymt-message" dir="rtl">
        <div class="paymt-message__figure"></div>
        <div class="paymt-message__text">@(ViewBag.IsSuccess ? "عملية دفع ناجحة" : "عملية دفع غير ناجحة")</div>
        <hr />
        @if (Model != null)
        {
            <table>
                <tr>
                    <td>المبلغ المدفوع</td>
                    <td>@Model.FormattedAmount</td>
                </tr>
                @if (Model.Source != null)
                {
                    <tr>
                        <td>الشركة</td>
                        <td>@(((Moyasar.Models.CreditCard)Model.Source).Company)</td>
                    </tr>
                    <tr>
                        <td>رقم البطاقة</td>
                        <td>@(((Moyasar.Models.CreditCard)Model.Source).Number)</td>
                    </tr>
                    <tr>
                        <td>الاسم</td>
                        <td>@(((Moyasar.Models.CreditCard)Model.Source).Name)</td>
                    </tr>
                }
                <tr>
                    <td>الوصف</td>
                    <td>@Model.Description</td>
                </tr>
                <tr>
                    <td>رقم العملية</td>
                    <td>@ViewBag.TransactionNumber</td>
                </tr>
                <tr>
                    <td>تاريخ العملية</td>
                    <td>@Model.CreatedAt?.ToString("dd MMMM yyyy hh:mm tt", new CultureInfo("ar-EG"))</td>
                </tr>
            </table>
            <hr />
        }
        <div class="paymt-message__description">سيتم تحويلكم بعد <span id="seconds">7</span> ثواني</div>
    </div>

    <script>
        var seconds = 7;
        var timer;

        //TODO, return to mobile app
        function redirect() {
            document.location.href = document.location.href
        }

        function updateSecs() {
            document.getElementById("seconds").innerHTML = seconds;
            seconds--;
            if (seconds == -1) {
                clearInterval(timer);
                redirect();
            }
        }

        function countdownTimer() {
            timer = setInterval(function () {
                updateSecs()
            }, 1000);
        }

        countdownTimer();
    </script>
</body>
</html>
